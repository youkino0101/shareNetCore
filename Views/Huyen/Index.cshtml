@model IEnumerable<NguyenThanhQuan_QLThongTin_MVC.Models.Huyen>

@{
    ViewData["Title"] = "Index";
}

<form method="get" action="Index">
    <input type="text" name="key" />
</form>

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenHuyen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tinh)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenHuyen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tinh.TenTinh)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdHuyen">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdHuyen">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdHuyen">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
